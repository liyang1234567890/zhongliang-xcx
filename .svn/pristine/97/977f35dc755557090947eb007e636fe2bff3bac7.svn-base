/* pages/nabi/navi.wxss */
.touch-item {
    /* font-size: 14px; */
    display: flex;
    justify-content: space-between;
    border-bottom:16rpx solid #b8b8b8;
    /* width: 100%; */
    overflow: hidden;
   }
   .content {
    /* width: 100%; */
    /* padding: 10px; */
    /* line-height: 22px; */
    /* margin-right:0; */
    transition: all 0.4s;
    transform: translateX(90px);
    margin-left: -130rpx;
    /* display: flex;
    align-items: center */
   }
   .del {
    background-color: orangered;
    width: 90px;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    color: #fff;
    -webkit-transform: translateX(90px);
    transform: translateX(90px);
    -webkit-transition: all 0.4s;
    transition: all 0.4s;
   }
   .touch-move-active .content,
   .touch-move-active .del {
    -webkit-transform: translateX(0);
    transform: translateX(0);
   }
