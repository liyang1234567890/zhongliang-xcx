<!--pages/mine/category/category.wxml-->
<view id='category'>
  <view class='cate-cont'>
    <view class='wrapper' wx:for='{{category}}' wx:key='{{index}}'>
      <navigator url='cateSub/cateSub?index={{index}}' class='box'>
        <image src='../../../mock/images/category_{{index}}.png' class='cate-img'></image>
        <text>\n</text>
        <view>{{item.name}}</view>
      </navigator>
    </view>
  </view>
</view>