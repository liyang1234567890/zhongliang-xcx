page{
  font-size: 30rpx;
}
.header{
  height: 200rpx;
}
.header-box{
  line-height: 200rpx;
  height: 100%;
  position: relative;
  display: flex;
  justify-content: space-between;
  margin: 20rpx 40rpx;
  padding: 10rpx 20rpx;
}
.mFun{
  background: #efefef;
  margin-top: 40rpx; 
  padding: 30rpx 0;
  bottom: 0;
}
.box{  
   display: flex;
   justify-content: space-between;
   background-color: #fff;
   border: 2rpx solid #ccc;
   border-radius: 14rpx;
   margin: 20rpx 40rpx;
   padding: 10rpx 20rpx;
}
.light{
  font-size: 28rpx;
  color: #ccc;
}
.mImg{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  position: absolute;
  top: 50%; 
  right: 0;
  transform: translateY(-50%); 
}
.setMsg{
  font-size: 32rpx;
  height: 80rpx;
  line-height: 80rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  border-radius: 20rpx;    
  width: 90%;
  bottom: 20rpx;  
  background: #1AAD19;
}
.setText{
  text-align: center;
  color: #fff;
}
